{
	"hosting": {
		"public": "public",
		"rewrites": [{
			"source": "**",
			"destination": "/index.html"
		}],
		"headers": [{
			"source": "**/*.@(jpg|gif|png)",
			"headers": [{
				"key": "Cache-Control",
				"value": "max-age=7200"
			}]
		},
    {
			"source": "**/*.@(js|css)",
			"headers": [{
				"key": "Cache-Control",
				"value": "max-age=7200"
			}]
		},
    {
      "source": "**/*.@(json)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=86400"
      }]
    }]
	}
}
